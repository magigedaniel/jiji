<script type="text/javascript">
	var whichToShow1 = Math.floor(Math.random() * $('#quotes1 .quote').length);
	$('#quotes1 .quote').hide().eq(whichToShow1).fadeIn(1000);
	
	var whichToShow2 = Math.floor(Math.random() * $('#quotes2 .quote').length);
	$('#quotes2 .quote').hide().eq(whichToShow2).fadeIn(1000);
	
$('.tab-pane').not('.active').hide();
	$('#apitab a').click(function (e) {
	  	e.preventDefault()
		$('#apitab li') .removeClass('active');
	  	$(this).parent('li').addClass('active');
	  	var id = $(this).attr('href');
	  	
		if(id === '#run'){
	  		alert('bingo you are trying to run these code hahahahaha!')
	  	}
	  	else{
	  		$('.tab-pane').hide().removeClass('in');
	  		$(id).show().addClass('in');
	  	}	  
	})
</script>
