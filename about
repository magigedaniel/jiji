/*
* Shuffle jQuery array of elements - see Fisher-Yates algorithm
*/
jQuery.fn.shuffle = function () {
    var j;
    for (var i = 0; i < this.length; i++) {
        j = Math.floor(Math.random() * this.length);
        $(this[i]).before($(this[j]));
    }
    return this;
};

$('#team img').shuffle();

$('#team img').on('mouseenter',function(){
    $('.imgOverlay').remove();
    var img = $(this);
    var name = img.attr('name');
    var imgp = img.position();
    var imgW = img.width();
    var imgH = img.height();
    if(name){
        var html = '<div class="cnt">\
        <div class="name">'+adb[name].name+'</div>\
        <div class="title">'+adb[name].title+'</div>\
        </div>';
        var overlay = $('<div/>',{
            img: name,
            class: 'imgOverlay',
            html: html
        })
        overlay.css({'position':'absolute', 'top':imgp.top, 'left':imgp.left, 'width':imgW, 'height':imgH})
        $('body').append(overlay);

        $(overlay).on('mouseleave',function(){
            $('.imgOverlay').remove();
        })

        $(overlay).on('click',function(){            
            var name = $(this).attr('img');
            var html = '<div class="desWrp">\
                <img src="https://msg91.com/images/aboutus/'+name+'-clr.jpg" style="width:500px; height:282px;" >\
                <div class="cntWrp">\
                    <div class="name">'+adb[name].name+'</div>\
                    <div class="title">'+adb[name].title+'</div>\
                    <div class="des">'+adb[name].des+'</div>\
                </div>\
            </div>'; 
            $.colorbox({html:html, opacity:.7, transition:'none', maxWidth:'500px'});
        })
    }
})
</script>

