$(document).ready(function(){
    
    var searchFlag = '1';
    if(searchFlag)
    {
        var searchCountry = 'Barbados';
       $("#search").val(searchCountry);
    }
    else{
    $("#search").val('Kenya');
    }
    //searchValueByIso('KE')
    getPrice();
    showBankDetail('KE');
    $('select').selectric({
        responsive: true
    });
})

  $('#submit').click(function() {
        var query = $('#query').val();
        if (query==null || query== "")
        {
          $('#query').attr('placeholder','Please enter query');
          $('#query').addClass( "error" );
          return false;
        }
        var mob_no = $('#mob_no').val();
        if (mob_no ==null || mob_no == "")
        {
          $('#mob_no').attr('placeholder','Please enter mobile number');
          $('#mob_no').addClass( "error" );
          return false;
        }
        var formdata = $("#mailtoceo").serialize();
        $.ajax({
            type: "POST",
            url: "../action_layer.php?action=contact",
            data: formdata,
            dataType: "json",
            success: function(data) {
                $('#msg').removeAttr('class');
                $('#msg').addClass(data.msgType).html(data.msg);
                setTimeout(function() { $("#msg").hide(); }, 5000);
                $('#mailtoceo')[0].reset();
            }
        })
    });
    
    var _globalTimeOut = null;
function searchBySMS() {
	if (_globalTimeOut != null)
		clearTimeout(_globalTimeOut);

	_globalTimeOut = setTimeout(function () {

        try{
            getPrice();
        }
        catch (e) {
            throw new Error(e.message);
        }

    }, 100);
}

$('#search').keyup(function(e) {
    $("#pricingData").html('<div class="ratesSpinner"></div>');
    if (e.keyCode == 13)
    {
        getPrice();
    }
});

function searchPrice(){
    //define some variables
    var country, countryCode, flagCode, flagPath;

    var term = $("#search").val();
    term = $.trim(term);
    if (term == '' || term == null){
    }

    $("#search").autocomplete({
        source: function(request, response)
        {
            autocompleteValue(request, response);
        },
        html: true,
        width: 490,
        matchContains: true,
        max: 25,
        delay: 600,
        scroll: true,
        scrollHeight: "auto",
        minChars: 2,
        multiple: false,
        mustMatch: false,
        autoFill: false,
        selectFirst: true,
        search: function(event, ui) {
            currency = $("#currency").val();
        },
        select: function(event, ui)
        {
            var regex = /^[0-9]+$/;
            if (!term.match(regex)) {
                country = ui.item.label;
                countryCode = ui.item.code;
            } 
            else {
                country = ui.item.label;
                countryCode = term;
            }
            $("#search").val(country);
            getPrice(country,countryCode);
        }
    })
            ._renderItem = function(ul, item)
            {
                return $().appendTo(ul);
            };


}

function getPrice(country,countryCode){
    country = $("#search").val();
    var currency = $('#currency').val();
    var routeType = $('#routeType').val();
    var noOfSMS = $('#noofsms').val();
    $.ajax({
        type: "GET",
        url: "https://control.JIJICOM.com/action_layer.php?action=511&request=pricing_details",
        dataType: 'jsonp',
        data: {"country": country, "countryCode": countryCode,"currency":currency,"routeType":routeType,"noOfSMS":noOfSMS},
        success: function(data) {
            showpricingData(data, currency);
        }
    });
}
function showpricingData(data, currency)
{
    $('#selectedcurrency').text(currency);
    $('#credit').text(data.credit);
    $('#creditAmount').text(data.creditAmount);
    $('#amountPerSms').text(data.amountPerSms);
    if(data.totalAmount <= 0)
    {
        $('#netAmount').text(1);
        
    }
    else
    {
        $('#netAmount').text(data.totalAmount);
    }
    $('#netAmtCurr').text(currency);
    
}

function autocompleteValue(request, response) {
    $.ajax({
        type: "GET",
        dataType: 'jsonp',
        url: "https://control.JIJICOM.com/action_layer.php?action=511&request=getPricingCountry",
        data: "term=" + request.term,
        success: function(data)
        {
            var arr = [];
            var i = 0;
            $.each(data, function(key, item)
            {
                arr[i] = {
                    value: item,
                };
                i++;
            })
            response(arr);
        }

    })



}

</script>
