function showBankDetail($ipcountry){
    
    if($ipcountry == 'IN'){
        //creating some templates for desktop or mobile
var bnktemp, mblBankTemp; 
bnktemp = '<thead><tr><th>Bank</th><th>IFSC Code</th><th class="third">Account Number</th><th>Account Name</th><th class="last">Address</th></tr></thead>';
bnktemp += '<tbody>';
	for (var bank in bankDetail)
	  {
	  bnktemp += '<tr>\
	  <td>'+bankDetail[bank].name+'</td>\
	  <td>'+bankDetail[bank].ifsc+'</td>\
	  <td>'+bankDetail[bank].acno+'</td>\
	  <td>'+bankDetail[bank].accountName+'</td>\
	  <td>'+bankDetail[bank].address+'</td>\
	  </tr>';
	  }
bnktemp += '</tbody>';
bnktemp +='<tr><td colspan="100%">Note: Please mention your JIJICOM username in description while making payment via bank transfer or cash deposit.</td></tr></tfoot>';
//bnktemp += '</tbody>';
//end of desktop template
//append it on table view				
$('#bnkDtlTbl').append(bnktemp);


//making a template for mobile version
mblBankTemp ='<div id="mblTblWrap">';
for (var bank in bankDetail)
	{
	mblBankTemp += '<table class="bnkDtlTbl" width="100%" border="0" cellpadding="0" cellspacing="0">\
	<thead><tr><th colspan="100%">'+bankDetail[bank].name+'</th></tr></thead>\
	<tbody>\
	<tr><td>IFSC Code</td><td>'+bankDetail[bank].ifsc+'</td></tr>\
	<tr><td>Account Number</td><td>'+bankDetail[bank].acno+'</td></tr>\
	<tr><td>Account Name</td><td>'+bankDetail[bank].accountName+'</td></tr>\
	<tr><td>Address</td><td>'+bankDetail[bank].address+'</td></tr>\
	</tbody>\
	</table>';
	}
mblBankTemp +='</div>';

//append it after main table
$('#bnkDtlTbl').after(mblBankTemp);
    }else
    {
        var str; 
        str = '<thead><tr><th>Bank</th><th>A/c Number</th><th >Routing Number</th><th>Swift Code</th><th >Branch</th><th>Beneficiary name</th></tr></thead>';
        str += '<tbody>';
        str += '<tr>\
	  <td>JP Morgan, Chase bank, N.A (802)</td>\
	  <td>618821578</td>\
	  <td>021202337</td>\
	  <td>CHASUS33</td>\
	  <td>Christopher Columbus-9780</td>\
          <td>WALKOVER,INC.</td>\
          </tr></tbody>';
       
        $('#bnkDtlTbl').html(str);
    }
}



