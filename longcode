<script type="text/javascript">
    function urlParam(name) {
        var results = new RegExp('[\?&]' + name + '=([^&#]*)').exec(window.location.href);
        if (results)
            return results[1];
        else
            return false;
    }

    var utmObj = {};

    (urlParam('utm_medium')) ? utmObj['utm_medium'] = urlParam('utm_medium') : '';
    (urlParam('utm_source')) ? utmObj['utm_source'] = urlParam('utm_source') : '';
    (urlParam('utm_campaign')) ? utmObj['utm_campaign'] = urlParam('utm_campaign') : '';
    (urlParam('utm_term')) ? utmObj['utm_term'] = urlParam('utm_term') : '';
    (urlParam('utm_content')) ? utmObj['utm_content'] = urlParam('utm_content') : '';
    
    var len = $.map(utmObj, function(n, i) { return i; }).length;

    if (len)
    {
        // Put the object into storage
        localStorage.setItem('utmParams', JSON.stringify(utmObj));
    }
    
    console.log(JSON.parse(localStorage.getItem('utmParams')));
</script>    <script type="text/javascript" src="js/jquery.form.js"></script>
    <script type="text/javascript" src="js/jquery.validate.min.js"></script>
    <script>
        //validation for sayhello form   
        $(function() {
            var tagCheckRE = new RegExp("(\\w.*)(\\s+)(\\w+)");
            jQuery.validator.addMethod("checkFullName", function(value, element) {
                return tagCheckRE.test(value);
            }, "Please enter valid full name");

            /// validate signup form on keyup and submit
            var validator = $("#longcode").validate({
                errorClass: 'formError',
                rules: {
                    //location: {required: true},
                    //signupCountry: {required: true},
                    name: {required: true, minlength: 3, checkFullName: true},
                    email: {required: true, email: true},
                    ccode: {required: true, number: true, minlength: 1, maxlength: 5},
                    mob_no: {required: true, number: true, minlength: 7, maxlength: 15}
                },
                messages: {
                    //                        location: {required: "Please Select Location First"},
                    //                        signupCountry: {required: "Please Select Location First"},
                    name: {
                        required: "Can't be blank.",
                        minlength: "Should Be Minimum 3 Character long"
                    },
                    ccode: {
                        required: "Can't be blank.",
                        number: "Please Enter Valid number.",
                        minlength: "Number must be 1 digit long."
                    },
                    mob_no: {
                        required: "Can't be blank.",
                        number: "Please Enter Valid number.",
                        minlength: "Number must be 7 digit long."
                    },
                    email: {
                        required: "Can't be blank.",
                        email: "Email is not valid"
                    }

                },
                // set this class to error-labels to indicate valid fields
                success: function(label) {
                },
                onkeyup: function(element) {
                    $(element).valid();
                },
                onfocusout: function(element) {
                    $(element).valid();
                },
            });

        });

        function validateReqFrom(formData, jqForm, options) {
            return jqForm.valid();
        }
        function showResponse(responseText, statusText, xhr, $form) {
            $("#longcode").hide();
            $("#sucMsg").show();
        }
        $(document).ready(function() {
            $('form').ajaxForm({beforeSubmit: validateReqFrom, success: showResponse});
        });

        $('#joinBtn').click(function(e) {
            e.preventDefault();
            var formdata = $("#longcode").serialize();
            $.ajax({
                type: "POST",
                url: "https://space.msg91.com/reseller_controller/longcodeUser?",
                async: false,
                //contentType: "application/json; charset=utf-8",
                data: formdata,
                dataType: "jsonp",
                success: function(data) {
                    if (data.msgType == "success")
                    {
                        $('#longcode')[0].reset();
                        $("#longcode").hide();
                        $("#sucMsg").show();
                    }
                    else
                    {
                        $('#msg').removeAttr('class');
                        $("#msg").show();
                        $('#msg').addClass(data.msgType).html(data.msg).delay(3000).fadeOut();
                    }
                }
            })
        });

    </script>
