<script>
//validations for sales form
       $(function() {
                var tagCheckRE = new RegExp("(\\w.*)(\\s+)(\\w+)");
                jQuery.validator.addMethod("checkFullName", function(value, element) {
                    return tagCheckRE.test(value);
                }, "Please enter valid full name");

/// validate signup form on keyup and submit
	  var validator = $("#sales").validate({ 
	  errorClass: 'formError',
	  rules: {
	  //location: {required: true},
	  //signupCountry: {required: true},
          name: {required: true, minlength: 3, checkFullName: true},
	  email: {required: true, email: true},
	  ccode: {required: true, number: true, minlength: 1, maxlength: 5},
	  mob_no: {required: true, number: true, minlength: 7, maxlength: 15}
	  },
	   messages: {
//                        location: {required: "Please Select Location First"},
//                        signupCountry: {required: "Please Select Location First"},
                        name: {
                            required: "Can't be blank.",
                            minlength: "Should Be Minimum 3 Character long"
                        },
                        ccode: {
                            required: "Can't be blank.",
                            number: "Please Enter Valid number.",
                            minlength: "Number must be 1 digit long."
                        },
                        mob_no: {
                            required: "Can't be blank.",
                            number: "Please Enter Valid number.",
                            minlength: "Number must be 7 digit long."
                        },
                        email: {
                            required: "Can't be blank.",
                            email: "Email is not valid"
                        }
                        
                       },
	
                    // set this class to error-labels to indicate valid fields
                    success: function(label) {
                    },
                    onkeyup: function(element) {
                        $(element).valid();
                    },
                    onfocusout: function(element) {
                        $(element).valid();
                    },
                });

            });

  </script>  
    
