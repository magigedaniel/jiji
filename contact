  <script>
      $(".succMsg").hide();
    //set a fake illustration
	$('#ccode , #mob_no').on('focus', function(){
		$(this).siblings().addClass('focus');
	});
	$("#ccode , #mob_no").focusout(function(){
		$(this).siblings().removeClass('focus');
	});
	
	
	function showhide(id, ths){
		$("#subCont li").removeClass('active')
		$(ths).addClass('active');
		$(".fhide").hide();
		$(id).show();
		}
            function validateReqFrom(formData, jqForm, options){
                if(jqForm.valid()){
                    jqForm.hide()
                    //jqForm.siblings(".succMsg").show();
                }
		return jqForm.valid();
            }
            function showResponse(responseText, statusText, xhr, $form)  { 
 
//    alert('status: ' + statusText + '\n\nresponseText: \n' + responseText + 
//        '\n\nThe output div should will be update with the responseText.'); 
} 
	$(document).ready(function() { 
            // bind 'myForm' and provide a simple callback function 
                $('form').ajaxForm({ beforeSubmit: validateReqFrom , success : showResponse });
           //
            //$('form').validate({errorClass: 'formError'})
		
        });
        
        function contactInfo(formId)
        {
            console.log("we are in this fun");
            var formdata = $("#"+formId).serialize();
            console.log(formdata);
            $.ajax({
                type: "POST",
                url: "/action_layer.php?action=contact",
                async: false,
                //contentType: "application/json; charset=utf-8",
                data: formdata,
                dataType: "json",
                success: function(data) {
                    console.log(data);
                    if (data.msgType == "success"){
                            $(".succMsg").show();
                    }
                }
            })
        }
        
    </script>

