    <script type="text/javascript">

        /* ===================== This function is used for show Notification======== */
        function show_message(type, message) {
            var t = 0;
            var nTemp = '<div class="' + type + '"><span>' + message + '</span></div>';
            $("#notification").html(nTemp);
            $("#notification").fadeIn();
            $(function() {
                t = setTimeout(function() {
                    $("#notification").fadeOut();
                }, 4000);
            });
            $("#notification").click(function() {
                // Slide it up right away
                $("#notification").fadeOut();
                // Stop the timeout that was running
                clearTimeout(t);
            });
        }
        ;



        /*form submit process start*/
        /*
         Author : @The Mechanic and @Rahul
         Date : 14 may 2014
         Purpose : want to make signup with ajax request and response in ajax
         Reviewer : @Rahul date 14 May 2014
         */
        $(document).ready(function() {


            $.validator.addMethod("checkFullName", function(value, element) {
                return /^[a-zA-Z0-9,]+\s+[a-zA-Z0-9,\s]+$/.test(value);
            }, "Please enter first name and last name.");

            $.validator.addMethod("CheckComName", function(value, element) {
                return /^[a-zA-Z0-9(),\s\_\@\.&]*$/.test(value);
            }, "Please enter a valid company name.");

            $.validator.addMethod("CheckUserName", function(value, element) {
                if (/[^a-zA-Z0-9\_\@\.]+/.test(value))
                    return false;
                else
                    return true;
            }, "Not allowed special characters except @, _, .");

            $.validator.addMethod("intlTel", function(value, element) {
                var val = value.trim();
                var data = $(element).intlTelInput('getSelectedCountryData');
                var countryCode = data.dialCode;

                var number = val.split("+" + countryCode)[1];
                if (number) {
                    number = number.replace(/\D/g, '');
                }
                var isValid = $(element).intlTelInput("isValidNumber");
                if (isValid) {
                    $("#user_country_code").val(countryCode);
                    $("#user_mobile_number").val(number);
                    return true;
                }
                else
                {
                    return false;
                }

            }, "Invalid mobile number");

            $("#signup").validate({
                errorClass: 'formError',
                rules: {
                    user_full_name: {required: true, minlength: 3, checkFullName: true},
                    user_name: {required: true, minlength: 5, CheckUserName: true, remote: {url: "/checkAvailablity.php", data: {"temp": Math.random()}}},
                    user_mobile_number: {required: true, minlength: 7, maxlength: 15},
                    mobile_number_display: {required: true, intlTel: true},
                    user_email: {required: true, email: true},
                    reference: {required: true},
                    TermsOfService: {required: true}
                },
                messages: {
                    user_full_name: {
                        required: "Can't be blank.",
                        minlength: "Should be minimum 3 characters long."
                    },
                    user_name: {
                        required: "Can't be blank.",
                        minlength: "Must be greater than 5 characters.",
                        remote: "This username is not available."
                    },
                    user_mobile_number: {
                        required: "Can't be blank.",
                        number: "Please enter a valid number.",
                        minlength: "Number must contain at least 7 digits."
                    },
                    mobile_number_display: {
                        required: "Can't be blank.",
                        intlTel: "Invalid mobile number"
                    },
                    user_email: {
                        required: "Can't be blank.",
                        email: "This Email ID is not valid."
                    },
                    reference: {
                        required: "Please select reference."
                    },
                    TermsOfService: {
                        required: "You must accept the terms of use."
                    }
                },
                // specifying a submitHandler prevents the default submit, good for the demo
                submitHandler: function(form) {
                    if ($(form).valid()) {
                        signUpSubmit();
                    }
                    else {
                        return false; // prevent normal form posting
                    }
                },
                onkeypress: function(element) {
                    $(element).valid();
                },
                onfocusout: function(element) {
                    $(element).valid();
                },
            });

            $("#strtMsg").prop("disabled", false);
        });

        //function shaker for add some shaking effect on button while error in 
        function StartShake() {
            $("#strtMsg").addClass("shaker");
            setTimeout(function() {
                $("#strtMsg").removeClass("shaker");
            }, 1000);
        }

        //function for add some animate method on submit button
        function addMethodTobtn() {
            $("#strtMsg").html('Processing <span class="three-quarters"></span>').addClass("active").prop("disabled", true);
        }
        //function for remove some animate method on submit button
        function removeMethodTobtn() {
            $("#strtMsg").html('Start Messaging').removeClass("active").prop("disabled", false);
        }

        // actually now sending via ajax or jsonp
        function signUpSubmit() {
            addMethodTobtn()
            var uri = "https://control.JIJICOM.com/action_layer.php?action=12";
            var url = uri + "&" + $("#signup").serialize();

            $.ajax({
                contentType: "application/json;charset=utf-8",
                url: url,
                data: JSON.parse(localStorage.getItem('utmParams')),
                crossDomain: true,
                dataType: 'jsonp',
                jsonpCallback: 'showResponse',
                jsonp: 'callback',
                error:function(jqXHR, textStatus, errorThrown){
                    console.log(jqXHR, textStatus, errorThrown);
                },
                success: function(data) {
                    console.log("process complete")
                }
            });
        }

        function setCookie(cname, cvalue) {
            var d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            var expires = "expires=" + d.toUTCString();
            document.cookie = cname + "=" + cvalue + "; " + expires;
        }

        function showResponse(data) {
            //remove animation from submit button
            removeMethodTobtn()
            //show message
            var showMsg = '';
            if (data.msg_type && data.msg_arr) {
                $.each(data.msg_arr, function(field, msg) {
                    if (showMsg == '') {
                        showMsg = msg;
                    }
                });
                show_message(data.msg_type, showMsg);
            }
            //now if message is success then redirect it to sign in page
            if (data.msg_type == "success") {
                var redUrl = $('#redirectUrl').val();
                var userName = $('#user_name').val();
                var mobile = $('#user_country_code').val() + $('#user_mobile_number').val();

                document.cookie = "signup=true";
                localStorage.removeItem('utmParams');
                setTimeout(function() {
                    if (0 == 1) {
                        redUrl = redUrl + "&mobile=" + mobile + "&username=" + userName;
                        window.location.href = redUrl;
                        return false;
                    }
                    window.location.href = redUrl;
                    return false;
                }, 4000);
            }
        }


        $(document).ready(function() {
            //set focus after page load
            $('#fname').focus();
            //set a fake illustration
            $('#ccode , #mob_no').on('focus', function() {
                $(this).siblings().addClass('focus');
            });
            $("#ccode , #mob_no").focusout(function() {
                $(this).siblings().removeClass('focus');
            });
        });
        
        var iso = $("#countryCode").val();
        $("#mobile_number_display").intlTelInput({
            defaultCountry: iso,
            nationalMode: false,
            autoFormat: true,
            autoPlaceholder: true            
        });

        $("#mobile_number_display").attr('placeholder', intlTelInputUtils.getExampleNumber(iso, 0, 1));
